{
  "name": "annotate-uncovered-lines-action",
  "version": "3.1.1",
  "main": "dist/index.js",
  "author": "Smartly.io Solutions Oy",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c eslint.config.js .",
    "fixture:pack": "rm -f fixture.tgz && tar czf fixture.tgz test-fixture",
    "fixture:setup": "rm -rf test-fixture && tar xzf fixture.tgz && yarn --cwd test-fixture install && yarn --cwd test-fixture jest -c jest.config.js --coverage",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "clean": "rm -rf dist",
    "prebundle": "npm run clean",
    "bundle": "ncc build src/index.ts --source-map --license licenses.txt --target es2022",
    "pretest": "npm run fixture:setup",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "npm run bundle && git add -f -- dist/index.js"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "drange": "2.0.1",
    "parse-lcov": "^1.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0"
  }
}
